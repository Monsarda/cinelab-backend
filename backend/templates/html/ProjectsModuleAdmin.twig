{% extends 'base_wrap.twig' %}

{% block content %}

<div class="page-content-wrapper">
    <!-- BEGIN CONTENT BODY -->
    <div class="page-content">
        <!-- BEGIN PAGE HEADER-->
        
        <!-- BEGIN PAGE BAR -->
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li>
                    <a href="index.html">Home</a>
                    <i class="fa fa-circle"></i>
                </li>
                <li>
                    <span>{{page|capitalize}} Page</span>
                </li>
            </ul>
            
        </div>
        <!-- END PAGE BAR -->
        <!-- BEGIN PAGE TITLE-->
        <h1 class="page-title">{{page|capitalize}} Page
            <small>configuration</small>
        </h1>
        <!-- END PAGE TITLE-->
        <!-- END PAGE HEADER-->
        <ul>
        {% for proj in projects %}
            
                

                <li style="clear:both; margin-bottom: 15px;">
                    <div class="span" style="float:left;width: 150px;margin-right: 15px;">
                        <img src="/templates/img/projects/{{proj.poster}}" width="100%" alt="">
                    </div>
                    {{proj.title}}<br>
                    {{proj.director}}<br>
                    {{proj.dop}}<br>
                    {{proj.cinelab}}<br>
                    {{proj.year}}<br>
                    {{proj.cinelab}}
                    <div style="clear:both"></div>
                </li>
            
        {% endfor %}
        </ul>

        
    
    </div>
    <!-- END CONTENT BODY -->
</div>

{% endblock %}